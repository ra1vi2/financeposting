						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem groupName="__$INTERNAL$" name="RecKeyOp" label="KeyOp" visibleInFilterBar="true">
						<fb:control>
							<m:Input name="KeyOp" value="{FcnJonVHFilter>/RecKeyOp}"/>
