					</fb:FilterGroupItem>
					<fb:FilterGroupItem  groupName="__$INTERNAL$" name="RecPosnr" label="WBS element" visibleInFilterBar="true">
						<fb:control>
							<m:Input  name="Posnr" value="{FcnJonVHFilter>/RecPosnr}"/>
