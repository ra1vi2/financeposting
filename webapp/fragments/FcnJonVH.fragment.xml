					</fb:FilterGroupItem>
					<fb:FilterGroupItem  groupName="__$INTERNAL$" name="RecShop" label="Shop" visibleInFilterBar="true">
						<fb:control>
							<m:Input name="Shop" value="{FcnJonVHFilter>/RecShop}"/>
