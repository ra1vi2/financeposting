<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Table class="sapUiMediumMarginTop" items="{selectedOrgSDN>/}">
		<infoToolbar>
			<OverflowToolbar>
				<Label text="{i18n>table2HeaderText}" class="sapUiLargeMarginBegin"/>
			</OverflowToolbar>
		</infoToolbar>
		<columns>
			<Column >
				<Text text="FCN-JON"/>
			</Column>
			<Column>
				<Text text="ICN-JON"/>
			</Column>
			<Column>
				<Text text="KEYOP"/>
			</Column>
			<Column>
				<Text text="SHOP"/>
			</Column>
			<Column>
				<Text text="TS/TSD"/>
			</Column>
			<Column>
				<Text text="WBS Element"/>
			</Column>
			<Column>
				<Text text="Quantity"/>
			</Column>
			<Column>
				<Text text="Amount"/>
			</Column>
		</columns>
		<items>
			<ColumnListItem vAlign="Middle">
				<cells>
					<Text text="{selectedOrgSDN>FcnJon}"/>
					<Text text="{selectedOrgSDN>IcnJon}"/>
					<Text text="{selectedOrgSDN>KeyOp}"/>
					<Text text="{selectedOrgSDN>Shop}"/>
					<Text text="{selectedOrgSDN>Ts}/{selectedOrgSDN>Tsd}"/>
					<Text text="{selectedOrgSDN>Posnr}"/>
					<Text text="{selectedOrgSDN>SendQty}"/>
					<Text text="{selectedOrgSDN>SendAmt}"/>
				</cells>
			</ColumnListItem>
		</items>
	</Table>
</core:FragmentDefinition>